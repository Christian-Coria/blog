{
    "name": "BlogISPC",
    "description": "Blog de y para estudiantes del Instituto Politecnico Cordoba",
    "repository": "https://github.com/Christian-Coria/ispc-estudiantes",
    "logo": "https://cdn-icons-png.flaticon.com/512/5765/5765668.png",
    "keywords": ["django", "cms"],
    "env": {
      "SECRET": {
        "description": "RANDOM SECRET.",
        "generator": "secret"
      },
      "ALLOWED_HOST": {
        "description": "Leave the * or your-app-name.herokuapp.com",
        "value": "*"
      }
    },
    "addons": [
      {
        "plan": "heroku-postgresql",
        "options": {
          "version": "11"
        }
      }
    ]
  }